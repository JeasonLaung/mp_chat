<!--components/list/index.wxml-->
<navigator class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
        <image 
        wx:if="{{icon}}"
        src="{{icon}}" 
        style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
    </view>
    <view class="weui-cell__bd">{{title}}</view>
    <view class="weui-cell__ft {{isLink ? 'weui-cell__ft_in-access' : ''}}">{{value}}</view>
</navigator>